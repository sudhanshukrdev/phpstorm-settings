<keymap version="1" name="Visual Studio copy" parent="Visual Studio">
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="alt 1" />
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="FileChooser.TogglePathShowing" />
  <action id="GotoFile">
    <keyboard-shortcut first-keystroke="ctrl p" />
  </action>
  <action id="HideActiveWindow">
    <keyboard-shortcut first-keystroke="ctrl b" />
  </action>
  <action id="Print" />
  <action id="ShowBookmarks">
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="w" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl w" />
  </action>
  <action id="ViewMainMenu">
    <mouse-shortcut keystroke="control button1 doubleClick" />
  </action>
</keymap>